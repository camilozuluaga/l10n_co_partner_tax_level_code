<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="l10n_co_partner_tax_level_code_form" model="ir.ui.view">
            <field name="name">l10n.co.partner.tax.level.code.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="l10n_co_partner_vat.view_partner_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='vat']" position="after">
                    <field name="tax_level_code_id"/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>
